{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "0.8.1",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "run-s package:sync package:pkg package:rollup package:lint",
		"package:sync": "svelte-kit sync",
		"package:pkg": "svelte-package",
		"package:rollup": "rollup --file dist/rollup-plugin.cjs --format cjs dist/rollup-plugin.js",
		"package:lint": "publint",
		"preview": "vite preview",
		"prepare": "husky install && svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest",
		"prepublishOnly": "npm run package"
	},
	"dependencies": {
		"@fluent/dom": "^0.10.0",
		"@fluent/sequence": "^0.8.0",
		"cached-iterable": "^0.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "19.4.1",
		"@commitlint/config-conventional": "19.4.1",
		"@fluent/bundle": "0.18.0",
		"@fluent/langneg": "0.7.0",
		"@fontsource-variable/manrope": "5.0.21",
		"@iconify-json/mdi": "1.2.1",
		"@sveltejs/adapter-static": "3.0.4",
		"@sveltejs/kit": "2.7.0",
		"@sveltejs/package": "2.3.4",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@testing-library/svelte": "5.1.0",
		"@types/hast": "3.0.4",
		"@types/jsdom": "21.1.7",
		"@types/semver": "7.5.8",
		"@typescript-eslint/eslint-plugin": "8.8.1",
		"@typescript-eslint/parser": "8.8.1",
		"@vitest/ui": "2.0.5",
		"autoprefixer": "10.4.20",
		"eslint": "9.12.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.43.0",
		"globals": "15.9.0",
		"hast-util-heading-rank": "3.0.0",
		"hast-util-to-string": "3.0.0",
		"husky": "9.1.5",
		"lint-staged": "15.2.10",
		"mdsvex": "0.12.3",
		"npm-run-all2": "6.2.2",
		"pathe": "1.1.2",
		"postcss": "8.4.47",
		"postcss-load-config": "6.0.1",
		"postcss-preset-env": "10.0.2",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.7",
		"publint": "0.2.10",
		"rehype-add-classes": "1.0.0",
		"rehype-autolink-headings": "7.1.0",
		"rehype-external-links": "3.0.0",
		"rehype-shift-heading": "2.0.0",
		"rehype-slug": "6.0.0",
		"rehype-wrap-all": "1.1.0",
		"semver": "7.6.3",
		"shiki": "1.22.0",
		"svelte": "4.2.19",
		"svelte-check": "4.0.5",
		"svelte2tsx": "0.7.22",
		"tslib": "2.7.0",
		"typescript": "5.5.4",
		"typescript-eslint": "8.8.1",
		"unist-util-visit": "5.0.0",
		"unplugin-icons": "0.19.3",
		"vitest": "2.0.5"
	},
	"peerDependencies": {
		"jsdom": "^20.0.0 || ^21.0.0 || ^22.0.0 || ^23.0.0 || ^24.0.0 || ^25.0.0",
		"rollup": "^4.0.0",
		"svelte": "^3.55.0 || ^4.0.0",
		"vite": "^5.0.12"
	},
	"lint-staged": {
		"*.{cjs,js,ts,svelte}": [
			"eslint --cache --fix",
			"prettier --write"
		],
		"*.{md,json,yaml}": "prettier --write"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./rollup-plugin": {
			"types": "./dist/rollup-plugin.d.ts",
			"require": "./dist/rollup-plugin.cjs",
			"default": "./dist/rollup-plugin.js"
		},
		"./ssr": {
			"types": "./dist/ssr.d.ts",
			"default": "./dist/ssr.js"
		}
	},
	"files": [
		"dist",
		"src/lib"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		">4.0": {
			"rollup-plugin": [
				"./dist/rollup-plugin.d.ts"
			],
			"ssr": [
				"./dist/ssr.d.ts"
			]
		}
	},
	"packageManager": "pnpm@9.9.0"
}
