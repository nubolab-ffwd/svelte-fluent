{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "0.7.0",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "run-s package:sync package:pkg package:rollup package:lint",
		"package:sync": "svelte-kit sync",
		"package:pkg": "svelte-package",
		"package:rollup": "rollup --file dist/rollup-plugin.cjs --format cjs dist/rollup-plugin.js",
		"package:lint": "publint",
		"preview": "vite preview",
		"prepare": "husky install && svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest",
		"prepublishOnly": "npm run package"
	},
	"dependencies": {
		"@fluent/dom": "^0.9.0",
		"@fluent/sequence": "^0.8.0",
		"cached-iterable": "^0.3.0",
		"jsdom": "^23.0.0",
		"rollup": "^4.0.0",
		"vite": "^5.0.12"
	},
	"devDependencies": {
		"@commitlint/cli": "18.4.3",
		"@commitlint/config-conventional": "18.4.3",
		"@fluent/bundle": "0.18.0",
		"@fluent/langneg": "0.7.0",
		"@fontsource-variable/manrope": "5.0.18",
		"@iconify-json/mdi": "1.1.64",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "2.2.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/svelte": "4.0.5",
		"@types/hast": "3.0.4",
		"@types/jsdom": "21.1.6",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"@vitest/ui": "1.2.2",
		"autoprefixer": "10.4.17",
		"eslint": "8.55.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"glob": "10.3.10",
		"hast-util-heading-rank": "3.0.0",
		"hast-util-to-string": "3.0.0",
		"husky": "8.0.3",
		"lint-staged": "15.2.2",
		"mdsvex": "0.11.0",
		"npm-run-all2": "5.0.2",
		"postcss": "8.4.32",
		"postcss-load-config": "5.0.2",
		"postcss-preset-env": "9.3.0",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.1.2",
		"publint": "0.2.6",
		"rehype-add-classes": "1.0.0",
		"rehype-autolink-headings": "7.1.0",
		"rehype-external-links": "3.0.0",
		"rehype-shift-heading": "2.0.0",
		"rehype-slug": "6.0.0",
		"rehype-wrap-all": "1.1.0",
		"shiki": "0.14.7",
		"svelte": "4.2.8",
		"svelte-check": "3.6.2",
		"svelte2tsx": "0.6.27",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"unist-util-visit": "5.0.0",
		"unplugin-icons": "0.18.1",
		"vitest": "1.2.2"
	},
	"peerDependencies": {
		"jsdom": "^16.4.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0 || ^22.0.0 || ^23.0.0",
		"svelte": "^3.55.0 || ^4.0.0"
	},
	"lint-staged": {
		"*.{cjs,js,ts,svelte}": [
			"eslint --cache --ignore-path .gitignore --fix",
			"prettier --write"
		],
		"*.{md,json,yaml}": "prettier --write"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./rollup-plugin": {
			"types": "./dist/rollup-plugin.d.ts",
			"require": "./dist/rollup-plugin.cjs",
			"default": "./dist/rollup-plugin.js"
		},
		"./ssr": {
			"types": "./dist/ssr.d.ts",
			"default": "./dist/ssr.js"
		}
	},
	"files": [
		"dist",
		"src/lib"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
