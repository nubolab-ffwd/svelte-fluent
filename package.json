{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "0.4.3",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"prepare": "husky install && svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "run-s test:*",
		"test:dom": "vitest --run",
		"test:ssr": "vitest -c vitest.ssr.config.ts --run",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1"
	},
	"dependencies": {
		"@fluent/dom": "0.8.1",
		"@fluent/sequence": "0.7.0",
		"cached-iterable": "0.3.0",
		"jsdom": "21.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.4.4",
		"@commitlint/config-conventional": "17.4.4",
		"@fluent/bundle": "0.17.1",
		"@fluent/langneg": "0.6.2",
		"@fontsource/manrope": "4.5.13",
		"@iconify-json/mdi": "1.1.47",
		"@sveltejs/adapter-static": "2.0.1",
		"@sveltejs/kit": "1.7.1",
		"@sveltejs/package": "1.0.2",
		"@testing-library/svelte": "3.2.2",
		"@types/hast": "2.3.4",
		"@types/jsdom": "21.1.0",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"@vitest/ui": "0.28.5",
		"autoprefixer": "10.4.13",
		"eslint": "8.34.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"glob": "8.1.0",
		"hast-util-heading-rank": "2.1.1",
		"hast-util-to-string": "2.0.0",
		"husky": "8.0.3",
		"lint-staged": "13.1.2",
		"mdsvex": "0.10.6",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"postcss-load-config": "4.0.1",
		"postcss-preset-env": "8.0.1",
		"prettier": "2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"rehype-add-classes": "1.0.0",
		"rehype-autolink-headings": "6.1.1",
		"rehype-external-links": "2.0.1",
		"rehype-shift-heading": "1.0.2",
		"rehype-slug": "5.1.0",
		"rehype-wrap-all": "1.1.0",
		"sass": "1.58.2",
		"shiki": "0.14.1",
		"svelte": "3.55.1",
		"svelte-check": "3.0.3",
		"svelte2tsx": "0.6.1",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"unist-util-visit": "4.1.2",
		"unplugin-icons": "0.15.3",
		"vite": "4.1.2",
		"vite-plugin-static-copy": "0.13.0",
		"vitest": "0.28.5"
	},
	"peerDependencies": {
		"jsdom": "^16.4.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0"
	},
	"lint-staged": {
		"*.{cjs,js,ts,svelte}": [
			"eslint --cache --ignore-path .gitignore --fix",
			"prettier --write --plugin-search-dir=."
		],
		"*.{md,json,yaml}": "prettier --write --plugin-search-dir=."
	}
}
