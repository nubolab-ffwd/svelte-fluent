{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "0.4.3",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "husky install && svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"dependencies": {
		"@fluent/dom": "0.8.1",
		"@fluent/sequence": "0.7.0",
		"cached-iterable": "0.3.0",
		"jsdom": "20.0.0"
	},
	"devDependencies": {
		"@babel/core": "7.18.6",
		"@commitlint/cli": "17.0.3",
		"@commitlint/config-conventional": "17.0.3",
		"@fluent/bundle": "0.17.1",
		"@fluent/langneg": "0.6.2",
		"@sveltejs/adapter-auto": "1.0.0-next.60",
		"@sveltejs/kit": "1.0.0-next.375",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/svelte": "3.1.3",
		"@types/jsdom": "^16.2.14",
		"@typescript-eslint/eslint-plugin": "5.30.6",
		"@typescript-eslint/parser": "5.30.6",
		"eslint": "8.19.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"husky": "8.0.1",
		"jest": "28.1.3",
		"jest-environment-jsdom": "28.1.3",
		"jest-transform-svelte": "2.1.1",
		"lint-staged": "13.0.3",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^7.7.2",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"rollup": "^2.77.0",
		"sass": "^1.54.0",
		"svelte": "3.49.0",
		"svelte-check": "2.8.0",
		"svelte-preprocess": "4.10.7",
		"svelte2tsx": "^0.5.11",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "3.0.0"
	},
	"peerDependencies": {
		"jsdom": "^16.4.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0"
	},
	"lint-staged": {
		"*.{cjs,js,ts,svelte}": [
			"eslint --cache --ignore-path .gitignore --fix",
			"prettier --write --plugin-search-dir=."
		],
		"*.{md,json,yaml}": "prettier --write --plugin-search-dir=."
	}
}
