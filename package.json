{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "0.6.0",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package -o dist",
		"preview": "vite preview",
		"prepare": "husky install && svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "run-s test:*",
		"test:dom": "vitest --run",
		"test:ssr": "vitest -c vitest.ssr.config.ts --run",
		"prepublishOnly": "npm run package"
	},
	"dependencies": {
		"@fluent/dom": "^0.9.0",
		"@fluent/sequence": "^0.8.0",
		"cached-iterable": "^0.3.0",
		"jsdom": "^22.0.0",
		"rollup": "^3.18.0",
		"vite": "^4.1.4"
	},
	"devDependencies": {
		"@commitlint/cli": "17.6.3",
		"@commitlint/config-conventional": "17.6.3",
		"@fluent/bundle": "0.18.0",
		"@fluent/langneg": "0.7.0",
		"@fontsource/manrope": "5.0.2",
		"@iconify-json/mdi": "1.1.52",
		"@sveltejs/adapter-static": "2.0.2",
		"@sveltejs/kit": "1.20.0",
		"@sveltejs/package": "2.0.2",
		"@testing-library/svelte": "3.2.2",
		"@types/hast": "2.3.4",
		"@types/jsdom": "21.1.1",
		"@typescript-eslint/eslint-plugin": "5.59.7",
		"@typescript-eslint/parser": "5.59.7",
		"@vitest/ui": "0.31.4",
		"autoprefixer": "10.4.14",
		"eslint": "8.41.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"glob": "10.2.2",
		"hast-util-heading-rank": "2.1.1",
		"hast-util-to-string": "2.0.0",
		"husky": "8.0.3",
		"lint-staged": "13.2.2",
		"mdsvex": "0.10.6",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.24",
		"postcss-load-config": "4.0.1",
		"postcss-preset-env": "8.4.2",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"rehype-add-classes": "1.0.0",
		"rehype-autolink-headings": "6.1.1",
		"rehype-external-links": "2.1.0",
		"rehype-shift-heading": "1.0.2",
		"rehype-slug": "5.1.0",
		"rehype-wrap-all": "1.1.0",
		"shiki": "0.14.2",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte2tsx": "0.6.15",
		"tslib": "2.5.2",
		"typescript": "5.0.4",
		"unist-util-visit": "4.1.2",
		"unplugin-icons": "0.16.1",
		"vitest": "0.31.4"
	},
	"peerDependencies": {
		"jsdom": "^16.4.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0 || ^22.0.0",
		"svelte": "^3.55.0"
	},
	"lint-staged": {
		"*.{cjs,js,ts,svelte}": [
			"eslint --cache --ignore-path .gitignore --fix",
			"prettier --write --plugin-search-dir=."
		],
		"*.{md,json,yaml}": "prettier --write --plugin-search-dir=."
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./rollup-plugin": {
			"types": "./dist/rollup-plugin.d.ts",
			"default": "./dist/rollup-plugin.js"
		},
		"./ssr": {
			"types": "./dist/ssr.d.ts",
			"default": "./dist/ssr.js"
		}
	},
	"files": [
		"dist",
		"src/lib"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
