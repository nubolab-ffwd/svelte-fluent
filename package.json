{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "1.0.3",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "run-s package:sync package:pkg package:rollup package:lint",
		"package:sync": "svelte-kit sync",
		"package:pkg": "svelte-package",
		"package:rollup": "rollup --file dist/rollup.cjs --format cjs dist/rollup.js",
		"package:lint": "publint",
		"preview": "vite preview",
		"prepare": "husky install && svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "run-s test:*",
		"test:dom": "vitest run",
		"test:ssr": "TEST_SSR=1 vitest run",
		"prepublishOnly": "npm run package"
	},
	"dependencies": {
		"@fluent/dom": "^0.10.0",
		"@fluent/sequence": "^0.8.0",
		"@rollup/pluginutils": "^5.1.3",
		"cached-iterable": "^0.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "19.6.0",
		"@commitlint/config-conventional": "19.6.0",
		"@fluent/bundle": "0.18.0",
		"@fluent/langneg": "0.7.0",
		"@fontsource-variable/manrope": "5.1.1",
		"@iconify-json/mdi": "1.2.1",
		"@sveltejs/adapter-static": "3.0.6",
		"@sveltejs/kit": "2.17.1",
		"@sveltejs/package": "2.3.7",
		"@sveltejs/vite-plugin-svelte": "5.0.1",
		"@testing-library/svelte": "5.2.6",
		"@types/hast": "3.0.4",
		"@types/jsdom": "21.1.7",
		"@types/semver": "7.5.8",
		"@typescript-eslint/eslint-plugin": "8.23.0",
		"@typescript-eslint/parser": "8.23.0",
		"@vitest/ui": "2.1.8",
		"autoprefixer": "10.4.20",
		"eslint": "9.19.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.46.1",
		"globals": "15.13.0",
		"hast-util-heading-rank": "3.0.0",
		"hast-util-to-string": "3.0.1",
		"husky": "9.1.7",
		"lint-staged": "15.2.10",
		"mdsvex": "0.12.3",
		"npm-run-all2": "7.0.1",
		"pathe": "1.1.2",
		"postcss": "8.4.49",
		"postcss-load-config": "6.0.1",
		"postcss-preset-env": "10.1.1",
		"prettier": "3.4.2",
		"prettier-plugin-svelte": "3.3.2",
		"publint": "0.2.12",
		"rehype-add-classes": "1.0.0",
		"rehype-autolink-headings": "7.1.0",
		"rehype-external-links": "3.0.0",
		"rehype-shift-heading": "2.0.0",
		"rehype-slug": "6.0.0",
		"rehype-wrap-all": "1.1.0",
		"semver": "7.6.3",
		"shiki": "1.29.2",
		"svelte": "5.19.7",
		"svelte-check": "4.1.1",
		"svelte2tsx": "0.7.30",
		"tslib": "2.8.1",
		"typescript": "5.7.2",
		"typescript-eslint": "8.23.0",
		"unist-util-visit": "5.0.0",
		"unplugin-icons": "0.22.0",
		"vite": "6.0.9",
		"vitest": "2.1.8"
	},
	"peerDependencies": {
		"@fluent/bundle": "^0.18.0",
		"jsdom": "^20.0.0 || ^21.0.0 || ^22.0.0 || ^23.0.0 || ^24.0.0 || ^25.0.0",
		"rollup": "^4.0.0",
		"svelte": "^5.0.0",
		"vite": "^5.0.12 || ^6.0.0"
	},
	"lint-staged": {
		"*.{cjs,js,ts,svelte}": [
			"eslint --cache --fix",
			"prettier --write"
		],
		"*.{md,json,yaml}": "prettier --write"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./legacy": {
			"types": "./dist/legacy/index.d.ts",
			"svelte": "./dist/legacy/index.js",
			"default": "./dist/legacy/index.js"
		},
		"./rollup": {
			"types": "./dist/rollup.d.ts",
			"require": "./dist/rollup.cjs",
			"default": "./dist/rollup.js"
		},
		"./ssr": {
			"types": "./dist/ssr.d.ts",
			"default": "./dist/ssr.js"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		},
		"./vite": {
			"types": "./dist/vite.d.ts",
			"default": "./dist/vite.js"
		}
	},
	"files": [
		"dist",
		"src/lib"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		">4.0": {
			"legacy": [
				"./dist/legacy/index.d.ts"
			],
			"rollup": [
				"./dist/rollup.d.ts"
			],
			"ssr": [
				"./dist/ssr.d.ts"
			],
			"types": [
				"./dist/types/index.d.ts"
			],
			"vite": [
				"./dist/vite.d.ts"
			]
		}
	},
	"packageManager": "pnpm@9.15.0"
}
