{
	"name": "@nubolab-ffwd/svelte-fluent",
	"version": "1.0.4",
	"license": "MIT",
	"homepage": "https://github.com/nubolab-ffwd/svelte-fluent",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nubolab-ffwd/svelte-fluent.git"
	},
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "svelte-package -i src -w",
		"package": "run-s package:pkg package:rollup package:lint",
		"package:pkg": "svelte-package -i src",
		"package:rollup": "rollup --file dist/rollup.cjs --format cjs dist/rollup.js",
		"package:lint": "publint",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"prepublishOnly": "npm run package"
	},
	"dependencies": {
		"@fluent/dom": "^0.10.0",
		"@fluent/sequence": "^0.8.0",
		"@rollup/pluginutils": "^5.1.3",
		"cached-iterable": "^0.3.0"
	},
	"devDependencies": {
		"@fluent/bundle": "0.19.1",
		"@fluent/langneg": "0.7.0",
		"@sveltejs/package": "2.5.4",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@types/jsdom": "27.0.0",
		"autoprefixer": "10.4.21",
		"npm-run-all2": "8.0.4",
		"postcss": "8.5.6",
		"postcss-load-config": "6.0.1",
		"postcss-preset-env": "10.4.0",
		"publint": "0.3.13",
		"semver": "7.7.2",
		"svelte": "5.39.6",
		"svelte-check": "4.3.2",
		"svelte2tsx": "0.7.44",
		"tslib": "2.8.1",
		"typescript": "5.9.2",
		"vite": "7.1.7"
	},
	"peerDependencies": {
		"@fluent/bundle": "^0.18.0 || ^0.19.0",
		"jsdom": "^20.0.0 || ^21.0.0 || ^22.0.0 || ^23.0.0 || ^24.0.0 || ^25.0.0 || ^26.0.0 || ^27.0.0",
		"rollup": "^4.0.0",
		"svelte": "^5.0.0",
		"vite": "^5.0.12 || ^6.0.0 || ^7.0.0"
	},
	"imports": {
		"#markup-parser": {
			"types": "./dist/markup-parser/client.d.ts",
			"browser": "./dist/markup-parser/client.js",
			"default": "./dist/markup-parser/server.js"
		}
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./legacy": {
			"types": "./dist/legacy/index.d.ts",
			"svelte": "./dist/legacy/index.js",
			"default": "./dist/legacy/index.js"
		},
		"./rollup": {
			"types": "./dist/rollup.d.ts",
			"require": "./dist/rollup.cjs",
			"default": "./dist/rollup.js"
		},
		"./types": {
			"types": "./dist/types/index.d.ts"
		},
		"./vite": {
			"types": "./dist/vite.d.ts",
			"default": "./dist/vite.js"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"types": "./dist/client.d.ts",
	"typesVersions": {
		">4.0": {
			"legacy": [
				"./dist/legacy/index.d.ts"
			],
			"rollup": [
				"./dist/rollup.d.ts"
			],
			"types": [
				"./dist/types/index.d.ts"
			],
			"vite": [
				"./dist/vite.d.ts"
			]
		}
	},
	"packageManager": "pnpm@10.17.1"
}
